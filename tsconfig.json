{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["node_modules/@types", "src/images.d.ts"]
  },
  "include": ["src"]
}
